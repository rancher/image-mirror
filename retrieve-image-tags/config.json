{
  "flannel": {
    "images": [
      "flannel/flannel"
    ],
    "versionSource": "github-latest-release:flannel-io/flannel"
  },
  "flannel-cni-plugin": {
    "images": [
      "flannel/flannel-cni-plugin"
    ],
    "versionSource": "github-latest-release:flannel-io/cni-plugin"
  },
  "calico": {
    "images": [
      "quay.io/calico/apiserver",
      "quay.io/calico/cni",
      "quay.io/calico/ctl",
      "quay.io/calico/kube-controllers",
      "quay.io/calico/node",
      "quay.io/calico/pod2daemon-flexvol",
      "quay.io/calico/typha"
    ],
    "versionSource": "github-latest-release:projectcalico/calico"
  },
  "calico-operator": {
    "images": [
      "quay.io/tigera/operator"
    ],
    "versionSource": "github-latest-release:tigera/operator"
  },
  "cilium": {
    "images": [
      "quay.io/cilium/cilium",
      "quay.io/cilium/clustermesh-apiserver",
      "quay.io/cilium/hubble-relay",
      "quay.io/cilium/kvstoremesh",
      "quay.io/cilium/operator-aws",
      "quay.io/cilium/operator-azure",
      "quay.io/cilium/operator-generic"
    ],
    "versionSource": "github-latest-release:cilium/cilium"
  },
  "cilium-certgen": {
    "images": [
      "quay.io/cilium/certgen"
    ],
    "versionSource": "github-latest-release:cilium/certgen"
  },
  "cilium-hubble-ui": {
    "images": [
      "quay.io/cilium/hubble-ui",
      "quay.io/cilium/hubble-ui-backend"
    ],
    "versionSource": "github-latest-release:cilium/hubble-ui"
  },
  "traefik": {
    "images": [
      "library/traefik"
    ],
    "versionSource": "registry",
    "versionFilter": "^2\\.[0-9]+\\.[0-9]+$",
    "latest": true
  },
  "sonobuoy": {
    "images": [
      "sonobuoy/sonobuoy"
    ],
    "versionSource": "github-latest-release:vmware-tanzu/sonobuoy"
  },
  "prom2teams": {
    "images": [
      "idealista/prom2teams"
    ],
    "versionSource": "github-latest-release:idealista/prom2teams"
  },
  "vsphere-csi": {
    "images": [
      "gcr.io/cloud-provider-vsphere/csi/release/driver",
      "gcr.io/cloud-provider-vsphere/csi/release/syncer"
    ],
    "versionSource": "github-latest-release:kubernetes-sigs/vsphere-csi-driver"
  },
  "csi-attacher": {
    "images": [
      "registry.k8s.io/sig-storage/csi-attacher"
    ],
    "versionSource": "github-latest-release:kubernetes-csi/external-attacher"
  },
  "csi-resizer": {
    "images": [
      "registry.k8s.io/sig-storage/csi-resizer"
    ],
    "versionSource": "github-latest-release:kubernetes-csi/external-resizer"
  },
  "storage-livenessprobe": {
    "images": [
      "registry.k8s.io/sig-storage/livenessprobe"
    ],
    "versionSource": "github-latest-release:kubernetes-csi/livenessprobe"
  },
  "csi-provisioner": {
    "images": [
      "registry.k8s.io/sig-storage/csi-provisioner"
    ],
    "versionSource": "github-latest-release:kubernetes-csi/external-provisioner"
  },
  "csi-driver-registrar": {
    "images": [
      "registry.k8s.io/sig-storage/csi-node-driver-registrar"
    ],
    "versionSource": "github-latest-release:kubernetes-csi/node-driver-registrar"
  },
  "cpi-release-manager": {
    "images": [
      "gcr.io/cloud-provider-vsphere/cpi/release/manager"
    ],
    "versionSource": "registry",
    "versionFilter": "^v1\\.[2-9]+\\.[3-9]+$"
  }
}

